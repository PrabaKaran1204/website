  "main_section": null,
  "main_section_html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Theme Selection</title>\n    <style>\n        /* Your existing CSS styles */\n        .theme-grid {\n            display: grid;\n            grid-template-columns: repeat(auto-fill, minmax(150px, 1fr));\n            gap: 20px;\n            padding: 20px;\n        }\n        .theme-button {\n            padding: 20px;\n            text-align: center;\n            border-radius: 8px;\n            font-size: 16px;\n            background-color: #000000;\n            color: #FFA500;\n            cursor: pointer;\n            transition: background-color 0.3s, color 0.3s, transform 0.3s;\n            border: 1px solid #ccc;\n        }\n        .theme-button:hover {\n            background: linear-gradient(45deg, red, orange);\n            color: white;\n            transform: scale(1.1);\n        }\n        .custom-alert {\n            display: none;\n            position: fixed;\n            top: 20%;\n            left: 50%;\n            transform: translate(-50%, -50%);\n            background-color: black;\n            color: orange;\n            padding: 20px;\n            border-radius: 8px;\n            font-size: 18px;\n            text-align: center;\n            z-index: 1000;\n            box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.5);\n        }\n        .show-alert {\n            display: block;\n            animation: fadeInOut 2s forwards;\n        }\n        @keyframes fadeInOut {\n            0%, 100% { opacity: 0; }\n            20%, 80% { opacity: 1; }\n        }\n    </style>\n</head>\n<body>\n\n<div class=\"theme-selection\">\n    <button id=\"choose-theme-btn\" class=\"btn btn-primary\" onclick=\"fetchThemes()\">Choose Theme</button>\n</div>\n\n<div id=\"theme-list\" style=\"display:none; margin-top: 10px;\">\n    <label>Select a Theme:</label>\n    <div id=\"theme-buttons\" class=\"theme-grid\"></div>\n</div>\n\n<div id=\"custom-alert\" class=\"custom-alert\">\n    Theme applied successfully!\n</div>\n\n<script>\n// Function to check if the user is an admin\nfunction checkIfAdmin() {\n    frappe.call({\n        method: \"online.public.api.is_user_admin\", // Replace with your actual module name\n        callback: function(response) {\n            if (!response.message) {\n                document.getElementById('choose-theme-btn').style.display = 'none'; // Hide the button\n            }\n        }\n    });\n}\n\n// Call the function when the DOM is fully loaded\ndocument.addEventListener(\"DOMContentLoaded\", function() {\n    checkIfAdmin();\n});\n\n// Function to fetch themes from the backend\nfunction fetchThemes() {\n    console.log('Fetching themes...');\n    frappe.call({\n        method: \"online.public.api.get_all_themes\",\n        callback: function(response) {\n            var themeButtonsContainer = document.getElementById('theme-buttons');\n            var themes = response.message;\n\n            // Clear previous buttons\n            themeButtonsContainer.innerHTML = '';\n\n            // Populate with theme buttons\n            themes.forEach(function(theme) {\n                var button = document.createElement('button');\n                button.className = 'theme-button';\n                button.textContent = theme.name;\n\n                // Event listener to handle theme application\n                button.onclick = function() {\n                    applyTheme(theme.name);\n                };\n\n                themeButtonsContainer.appendChild(button);\n            });\n\n            // Show the theme list div\n            document.getElementById('theme-list').style.display = 'block';\n        }\n    });\n}\n\n// Function to save the selected theme\nfunction applyTheme(selectedTheme) {\n    if (selectedTheme) {\n        frappe.call({\n            method: 'online.public.api.save_selected_theme',\n            args: {\n                theme_name: selectedTheme\n            },\n            callback: function(response) {\n                if (response.message === 'success') {\n                    showCustomAlert();\n                    setTimeout(function() {\n                        location.reload();\n                    }, 2000);\n                } else {\n                    alert('Failed to apply theme.');\n                }\n            }\n        });\n    }\n}\n\n// Function to show custom alert\nfunction showCustomAlert() {\n    var alertBox = document.getElementById('custom-alert');\n    alertBox.classList.add('show-alert');\n\n    setTimeout(function() {\n        alertBox.classList.remove('show-alert');\n    }, 2000);\n}\n</script>\n\n\n</body>\n</html>\n",
