  "main_section_html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Automatic Image Slideshow with Text Overlay</title>\n    <style>\n   /* General reset and font styles */\n* { box-sizing: border-box; }\nbody { \n    font-family: Arial, sans-serif; \n    margin: 0; \n    padding: 0; \n}\n\n/* Slideshow Container */\n.slideshow-container {\n    max-width: 100%;\n    position: relative;\n    margin: auto;\n    overflow: hidden;\n}\n\n/* Slide Image */\n#img1 {\n    width: 100%;\n    height: 85vh;\n}\n\n/* Dots (Pagination) */\n.dots {\n    text-align: center;\n    padding: 10px;\n}\n.dots span {\n    cursor: pointer;\n    height: 15px;\n    width: 15px;\n    margin: 0 2px;\n    background-color: #bbb;\n    border-radius: 50%;\n    display: inline-block;\n    transition: background-color 0.6s ease;\n}\n.dots .active {\n    background-color: #717171;\n}\n\n/* Navigation Arrows */\n.prev, .next {\n    cursor: pointer;\n    position: absolute;\n    top: 50%;\n    padding: 16px;\n    color: white;\n    font-weight: bold;\n    font-size: 18px;\n    transition: 0.6s ease;\n    border-radius: 0 3px 3px 0;\n    user-select: none;\n}\n.next {\n    right: 0;\n    border-radius: 3px 0 0 3px;\n}\n.prev:hover, .next:hover {\n    background-color: rgba(0, 0, 0, 0.8);\n}\n\n/* Product Grid */\n.product-grid1 {\n    display: grid;\n    grid-template-columns: repeat(3, 1fr);\n    gap: 20px;\n    width: 100%;\n    padding: 10px;\n}\n\n/* Product Card */\n.product-card1 {\n    text-align: center;\n    border: 1px solid #ddd;\n    padding: 20px;\n    border-radius: 10px;\n    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\n    background-color: #FFFFFF;\n}\n\n.product-card1 img {\n    width: 80%;\n    height: 60vh;\n    object-fit: cover;\n    border-radius: 10px;\n}\n\n/* Title Styling */\nh1, p {\n    font-family: 'Mamirath', sans-serif;\n    color: gray;\n    text-align: center;\n    margin: 10px 0;\n}\n\n/* Button Styling */\n#anchor {\n    background-color: gray;\n    color: white;\n    border: none;\n    padding: 10px 20px;\n    font-size: 16px;\n    cursor: pointer;\n    border-radius: 5px;\n}\n#anchor:hover {\n    background-color: darkgray;\n}\n\n/* Mobile Responsiveness */\n@media (max-width: 768px) {\n    #img1 {\n        height: 50vh; /* Decrease height for smaller screens */\n    }\n    .product-grid1 {\n        grid-template-columns: 1fr; /* Single column layout for mobile */\n    }\n    .product-card1 {\n        padding: 10px;\n    }\n    .prev, .next {\n        font-size: 14px;\n        padding: 10px;\n    }\n    h1 {\n        font-size: 24px; /* Adjust heading size */\n    }\n    p {\n        font-size: 16px; /* Adjust paragraph size */\n    }\n}\n\n/* For tablets and small desktops */\n@media (min-width: 768px) and (max-width: 1024px) {\n    .product-grid1 {\n        grid-template-columns: repeat(2, 1fr); /* Two columns for tablets */\n    }\n    h1 {\n        font-size: 28px;\n    }\n    p {\n        font-size: 18px;\n    }\n}\n\n\n    </style>\n</head>\n<body>\n\n<div class=\"slideshow-container\">\n    <!-- First Slide -->\n     <div class=\"mySlides\">\n        <img  id=\"img1\"src=\"/files/Simple Modern Photo Collage Autumn Fashion Sale Banner.png\" alt=\"Image 1\">\n   \n    </div>\n<!-- Second Slide -->\n\n <div class=\"mySlides\">\n    <img id=\"img1\"src=\"/files/Gray Minimalist New Collection Banner.jpg\" alt=\"Image 3\">\n    \n</div>\n    \n    \n    <!-- Third Slide -->\n\n   \n      <div class=\"mySlides\">\n        <img id =\"Beige Brown Minimal New year Sale Promotional Banner.png\" alt=\"Image 2\">\n      </div>\n  \n    \n\n\n\n    <a class=\"prev\" onclick=\"plusSlides(-1)\">&#10094;</a>\n    <a class=\"next\" onclick=\"plusSlides(1)\">&#10095;</a>\n</div>\n\n<div class=\"dots\">\n    <span class=\"dot\" onclick=\"currentSlide(1)\"></span>\n    <span class=\"dot\" onclick=\"currentSlide(2)\"></span>\n    <span class=\"dot\" onclick=\"currentSlide(3)\"></span>\n</div>\n\n<h1 style=\"font-family: 'Mamirath', sans-serif; font-size: 35px; color: gray; text-align: center;\">\n    Discover Our Curated Collection !!\n</h1>\n<p style=\"font-family: 'Mamirath', sans-serif; font-size: 30px; color: gray; text-align: center;\">\n   Elevate your wardrobe!!!!\n </p>\n <p style=\"font-family: 'Mamirath', sans-serif; font-size: 20px; color: gray; text-align: center;\">\n  quality!!!!\n </p>\n\n<div class=\"container1\">\n    <div class=\"product-grid1\">\n         <!--Product 1 -->\n        <div style=\"background-color: #FFFFFF;\" class=\"product-card1\">\n           <a   href=\"/dress/womens\"> <img src=\"http://127.0.1.1:8000/files/woman-6771288_1280.jpg\" alt=\"Product 1\"></a></br>\n           <h1 style=\"font-family: 'Mamirath', sans-serif; font-size: 15px; color: gray;\">Women</h1>\n\n        </div>\n\n           \n         <!--Product 2 -->\n      \n         <div class=\"product-card1\">\n           <a   href=\"/dress/men\"> <img src=\"http://127.0.1.1:8000/files/boy-4973056_1280.jpg\" alt=\"Product 3\"></a> </br>\n          <h1 style=\"font-family: 'Mamirath', sans-serif; font-size: 15px; color: gray;\">Men</h1>\n\n        </div>\n         <!--Product 3 -->\n           <div class=\"product-card1\">\n            <a  href=\"/dress/womens\"><img src=\"http://127.0.1.1:8000/files/pexels-abhishek-kumar-100706288-9419149c2ad96.jpg\" alt=\"Product 2\"></a> </br>\n            <h1 style=\"font-family: 'Mamirath', sans-serif; font-size: 15px; color: gray;\">Women</h1>\n\n        </div>\n        \n    </div>\n\n </div>\n \n\n<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n  <meta charset=\"UTF-8\">\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n  <style>\n   /* Parent container */\n.float-container {\n  width: 100%;\n  overflow: hidden; /* Clear floats */\n}\n\n/* Float child elements */\n.float-child {\n  float: left;\n  width: 50%; /* Each side takes up 50% of the container */\n  box-sizing: border-box; /* Include padding and border in the elementâ€™s width */\n}\n\n/* Text styling on the left side */\n.text-side {\n  padding: 20px;\n  background-color: #f8f9fa;\n  font-family: Arial, sans-serif;\n  font-size: 18px;\n  color: #333;\n  text-align: center; /* Center the text */\n}\n\n/* Image styling on the right side */\n.img-side {\n  padding: 80px;\n  background-color: #eaeaea;\n  text-align: center; /* Center the image */\n}\n\n/* Image container styling */\n.image-stack {\n  position: relative;\n  width: 300px; /* Set a fixed width for the container */\n  height: auto; /* Height will adjust based on content */\n}\n\n/* Inline image styling */\n#praba img {\n  width: 300px; /* Size of inline image */\n  height: auto; /* Maintain aspect ratio */\n}\n\n/* Mobile responsiveness: Stack the float-child elements and adjust padding on small screens */\n@media (max-width: 768px) {\n  /* Stack the children vertically */\n  .float-child {\n    width: 100%; /* Each child takes up the full width of the container */\n    float: none; /* Remove float on smaller screens */\n  }\n\n  /* Adjust text styling for smaller screens */\n  .text-side {\n    padding: 15px;\n    font-size: 16px; /* Reduce font size for readability */\n  }\n\n  /* Adjust image styling */\n  .img-side {\n    padding: 20px; /* Less padding on smaller screens */\n  }\n\n  /* Adjust the image container */\n  .image-stack {\n    width: 100%; /* Make image container full width on mobile */\n    padding: 10px; /* Adjust padding */\n  }\n\n  /* Inline image styling */\n  #praba img {\n    width: 100%; /* Make the image responsive to the container's width */\n    height: auto; /* Maintain aspect ratio */\n  }\n}\n\n/* Extra small screens (very mobile-friendly) */\n@media (max-width: 480px) {\n  .text-side {\n    font-size: 14px; /* Further reduce font size on very small screens */\n  }\n\n  .img-side {\n    padding: 10px; /* Less padding for very small screens */\n  }\n}\n\n  </style>\n</head>\n<body>\n\n<div class=\"float-container\">\n  <!-- Left side: text content -->\n  <div class=\"float-child text-side\">\n    <h2>Clothing Confidence</h2>\n    <p>\"Look good while doing good for the planet.\"\n\"Sustainability is always in style.\"\n\"Choose clothing that makes a difference.\"\n\"Fashion that cares for the world you wear it in.\"\n\"Eco-friendly fashion that fits your lifestyle</p>\n\n\n<img  src=\"/files/circle.webp\" alt=\"Clothing Image\">\n  <p>Clothing is very confident, representing your style and personality!</p>\n  </div>\n\n  <!-- Right side: image content -->\n  <div class=\"float-child img-side\">\n    <img src=\"/files/Light Blue Sci-Fi Futuristic Animated Logo.gif\" alt=\"Clothing Image\">\n  </div>\n</div>\n\n</body>\n</html>\n\n\n<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n  <meta charset=\"UTF-8\">\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n  <title>Full-Width Looping Video</title>\n  <style>\n    /* Make video span full width */\n    .full-width-video {\n      width: 100%;\n      height: auto;\n    }\n  </style>\n</head>\n<body>\n  <!-- Video setup with loop, autoplay, and full width -->\n  <video class=\"full-width-video\" controls autoplay loop muted>\n    <source src=\"/files/Brown and White Simple Animated Fashion New Arrival Promo Video.mp4\"type=\"video/mp4\">\n    Your browser does not support the video tag.\n  </video>\n</body>\n</html>\n<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n  <meta charset=\"UTF-8\">\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n  <title>Image and Text Side by Side</title>\n  <style>\n    /* Container for image and text */\n    .content-container {\n      display: flex;\n      justify-content: space-between;\n      align-items: center;\n      gap: 20px;\n      max-width: 80%;\n      margin: 20px auto;\n      padding: 10px;\n      border: 2px solid #ddd;\n      border-radius: 10px;\n      box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n    }\n\n    /* Image container */\n    .image-container {\n     width: 60%;\n\n      flex: 1;\n      height: 100px; /* Control cropping */\n      overflow: hidden; /* Hide overflow to crop image */\n      display: flex;\n      justify-content: center;\n      align-items: center;\n    }\n\n    /* Image styling */\n    .cropped-image {\n      width: 30%;\n      height: 100%;\n      object-fit: cover; /* Crop the image to fit the container */\n      object-position: center; /* Center the image within the container */\n    }\n\n    /* Unique style for h3 */\n    .unique-heading {\n      flex: 1;\n      font-family: 'Merriweather', serif; /* Elegant font */\n      font-size: 20px; /* Adjust size */\n      color: #4caf50; /* Green color for trust and quality */\n      text-align: left; /* Align text to the left */\n      line-height: 1.8; /* Improve readability */\n      letter-spacing: 1.2px; /* Slight spacing for a premium look */\n      text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.2); /* Add depth to the text */\n    }\n\n    /* Responsive design */\n    @media (max-width: 768px) {\n      .content-container {\n        flex-direction: column; /* Stack image and text */\n        text-align: center; /* Center text */\n      }\n\n      .image-container,\n      .unique-heading {\n        flex: unset;\n        width: 100%;\n      }\n    }\n  </style>\n</head>\n<body>\n  <div class=\"content-container\">\n    <!-- Image Container -->\n    <div class=\"image-container\">\n      <img src=\"/files/Dark Abstract Black Panther Gaming Logo.png\" alt=\"Cropped Image\" class=\"cropped-image\">\n    </div>\n\n    <!-- Text Content -->\n    <h3 class=\"unique-heading\">\n      Thank you for visiting our website!  \n      We trust in the quality of our clothing and the trust of our customers.\n    </h3>\n  </div>\n</body>\n</html>\n\n\n\n\n\n\n\n\n<script>\n    let slideIndex = 0;\n    showSlides();\n\n    function showSlides() {\n        let i;\n        const slides = document.getElementsByClassName(\"mySlides\");\n        const dots = document.getElementsByClassName(\"dot\");\n        for (i = 0; i < slides.length; i++) {\n            slides[i].style.display = \"none\";\n        }\n        slideIndex++;\n        if (slideIndex > slides.length) {slideIndex = 1}\n        for (i = 0; i < dots.length; i++) {\n            dots[i].className = dots[i].className.replace(\" active\", \"\");\n        }\n        slides[slideIndex - 1].style.display = \"block\";\n        dots[slideIndex - 1].className += \" active\";\n        setTimeout(showSlides, 3000); // Change image every 3 seconds\n    }\n\n    function plusSlides(n) {\n        slideIndex += n - 1;\n        showSlides();\n    }\n\n    function currentSlide(n) {\n        slideIndex = n - 1;\n        showSlides();\n    }\n</script>\n\n\n",
